{
  "name": "@codepunkt/codepunkt.de",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "1.0.0-beta.1",
  "author": "Christoph Werner <christoph@codepunkt.de>",
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "develop": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "GATSBY_TELEMETRY_DISABLED=1 gatsby serve",
    "clean": "GATSBY_TELEMETRY_DISABLED=1 gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.5",
    "@mdx-js/react": "^1.6.5",
    "framer-motion": "^1.11.0",
    "gatsby": "^2.22.15",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-linaria": "^2.0.0",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-mdx": "^1.2.15",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-remove-generator": "^1.0.5",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-sharp": "^2.5.3",
    "linaria": "^1.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "terser": "^4.7.0",
    "typeface-fira-code": "^1.1.4",
    "typeface-montserrat": "^0.0.75",
    "typeface-open-sans": "^0.0.75",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "gatsby-remark-vscode": "^2.1.2",
    "prettier": "2.0.5",
    "slack-theme": "slack-theme/visual-studio-code",
    "vscode-styled-components": "styled-components/vscode-styled-components"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
